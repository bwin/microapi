{
  "name": "@bwin/microapi",
  "version": "0.1.4",
  "license": "MIT",
  "author": "Benjamin Winkler [bwin]",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bwin/microapi"
  },
  "scripts": {
    "coverage": "yarn pretest && yarn nyc -n lib/ ava",
    "lint": "yarn coffeelint -q -f coffeelint.json src/",
    "prepare": "rm -r lib/ > /dev/null ; yarn coffee -o lib/ -c src/",
    "pretest": "rm -r .test/ > /dev/null ; yarn prepublish && yarn coffee -o .test/ -c test/",
    "test": "yarn ava",
    "testv": "yarn test -v",
    "testn": "yarn test -t | tap-nyan",
    "overview": "yarn lint && yarn coverage",
    "watch": "nodemon -e .coffee -w src/ -w test/ -x yarn overview"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "coffeelint": "^2.1.0",
    "coffeescript": "^2.4.1",
    "nodemon": "^1.18.11",
    "nyc": "^14.0.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "escape-string-regexp": "^2.0.0",
    "get-stream": "^5.1.0",
    "http-errors": "^1.7.2",
    "jsonwebtoken": "^8.5.1",
    "ms": "^2.1.1",
    "redis": "^2.8.0",
    "redis-lock": "^0.1.4",
    "strict-rate-limiter": "0.1.3",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "pmx": "^1.6.7"
  },
  "files": [
    "lib/"
  ],
  "ava": {
    "files": [
      ".test/specs/**/*.js"
    ]
  }
}
